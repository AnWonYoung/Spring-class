<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.koreait.spring.user.UserMapper">
<!-- 위 주소 (함께 사용할 인터페이스) 사용하기 -->

<!--  DAO에 적던 것  -->
    <insert id="insUser">
        INSERT INTO t_user
        (uid, upw, unm, gender)
        VALUES
        (#{uid}, #{upw}, #{unm}, #{gender})
    </insert>
</mapper>